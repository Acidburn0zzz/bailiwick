{
  "name": "bailiwick",
  "description": "This is an experimental model toolkit for web applications. It's meant to allow for a model that is closer to the Domain Model pattern.",
  "homepage": "https://bitbucket.org/ged/bailiwick#readme",
  "version": "0.1.0-alpha.5",
  "scripts": {
    "build": "WEBPACK_ENV=build webpack",
    "dev": "webpack --progress --colors --watch --mode=dev",
    "test": "karma start karma.conf.js --single-run"
  },
  "keywords": [
    "domain",
    "model",
    "fetch",
    "json",
    "rest"
  ],
  "author": "Michael Granger <ged@FaerieMUD.org>",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "hg",
    "url": "https://bitbucket.org/ged/bailiwick"
  },
  "directories": {
    "doc": "doc",
    "lib": "lib"
  },
  "format": "umd",
  "main": "lib/bailiwick.js",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-amd": "^6.6.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.6.0",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.6.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "eslint": "^3.11.1",
    "eslint-loader": "^1.6.1",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^0.3.7",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "webpack": "^1.13.3",
    "yargs": "^6.5.0"
  },
  "dependencies": {
    "bluebird": "^3.3.3",
    "es6-error": "^3.0.1",
    "inflection": "^1.8.0"
  }
}
